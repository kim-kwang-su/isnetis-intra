<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.isnet.mgr.dao.DeptDao">
	
	<select id="select_dept_01" resultType="hashmap">
		SELECT
			DEPT_NO, DEPT_NAME
		FROM
			TB_DEPT
		ORDER BY
			ORDER_NO
	</select>
	
</mapper>